{
  "name": "Snapchat",
  "terms": {
    "Terms of Service": {
      "fetch": "https://www.snap.com/en-GB/terms",
      "select": "main",
      "remove": [
        "header",
        "footer",
        ".cookie-landing-screen",
        "hr"
      ],
      "executeClientScripts": true
    },
    "Privacy Policy": {
      "fetch": "https://www.snap.com/en-GB/privacy/privacy-policy",
      "select": [
        "[data-testid=\"page\"]"
      ],
      "executeClientScripts": true
    },
    "Trackers Policy": {
      "fetch": "https://www.snap.com/en-GB/cookie-policy/",
      "executeClientScripts": true,
      "select": "[data-testid=\"page\"]"
    },
    "Community Guidelines": {
      "fetch": "https://www.snap.com/en-GB/community-guidelines",
      "select": [
        "[data-testid=\"page\"]"
      ],
      "executeClientScripts": true
    },
    "Content Monetisation Policy": {
      "combine": [
        {
          "fetch": "https://www.snap.com/monetization-terms",
          "select": [
            "[data-testid=\"page\"]"
          ],
          "remove": [
            "img"
          ]
        },
        {
          "fetch": "https://www.snap.com/terms/creator-stories",
          "select": [
            "[data-testid=\"page\"]"
          ],
          "remove": [
            "img"
          ]
        }
      ],
      "executeClientScripts": true
    },
    "Advertising Content Policy": {
      "fetch": "https://www.snap.com/ad-policies?lang=en-GB",
      "select": [
        "[data-testid=\"page\"]"
      ],
      "remove": [
        "img",
        "header",
        "footer",
        "li"
      ],
      "executeClientScripts": true
    }
  }
}